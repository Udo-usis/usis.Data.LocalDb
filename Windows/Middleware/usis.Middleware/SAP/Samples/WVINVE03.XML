              </xsd:simpleType>
             </xsd:element>
             <xsd:element name="VTEXT" minOccurs="0">
              <xsd:annotation>
               <xsd:documentation>Bezeichnung zu Layout</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"/>
