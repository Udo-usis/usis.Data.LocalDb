              </xsd:simpleType>
             </xsd:element>
             <xsd:element name="LTEXT" minOccurs="0">
              <xsd:annotation>
               <xsd:documentation>Layoutbereich-Bezeichnung</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"/>
