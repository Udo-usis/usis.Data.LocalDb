              </xsd:simpleType>
             </xsd:element>
             <xsd:element name="LAYVR" minOccurs="0">
              <xsd:annotation>
               <xsd:documentation>Layout</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                <xsd:maxLength value="10"/>
